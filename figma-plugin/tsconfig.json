{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": ".",
    "rootDir": ".",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["@figma/plugin-typings"]
  },
  "include": ["code.ts", "src/**/*.ts"],
  "exclude": ["node_modules"]
}
